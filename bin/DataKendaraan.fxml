<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" prefHeight="400.0" prefWidth="600.0" fx:controller="DataKendaraanController">
    <!-- Title Label -->
    <Label layoutX="200.0" layoutY="14.0" style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Data Kendaraan Terdaftar" />

    <!-- TableView for displaying vehicle data -->
    <TableView fx:id="vehicleTable" layoutX="20.0" layoutY="60.0" prefHeight="300.0" prefWidth="560.0">
        <columns>
            <TableColumn fx:id="nomorKendaraan" text="Nomor Kendaraan" prefWidth="140.0" />
            <TableColumn fx:id="jenisKendaraan" text="Jenis Kendaraan" prefWidth="140.0" />
            <TableColumn fx:id="namaPemilik" text="Nama Pemilik" prefWidth="140.0" />
            <TableColumn fx:id="kategoriPemilik" text="Kategori Pemilik" prefWidth="140.0" />
        </columns>
    </TableView>

    <!-- Button to return to admin menu -->
    <Button fx:id="btnKembali" text="Kembali ke Menu Admin" layoutX="20.0" layoutY="370.0"/>
</AnchorPane>